<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.echoed.chamber.dao.EventLogDao">

    <insert id="insert" parameterType="com.echoed.chamber.domain.EventLog">
    	insert into EventLog(
    			id,
    			createdOn,
    			name,
    			ref,
    			refId)
        values (
                #{id},
                #{createdOn},
                #{name},
                #{ref},
                #{refId})
    </insert>

</mapper>

