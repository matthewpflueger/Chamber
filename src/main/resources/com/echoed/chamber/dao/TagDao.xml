<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.echoed.chamber.dao.TagDao">

    <resultMap id="tagResult" type="com.echoed.chamber.domain.Tag">
        <constructor>
            <idArg column="tagId" javaType="String" />
        </constructor>
    </resultMap>

    <insert id="insert" parameterType="com.echoed.chamber.domain.Tag">
        insert into Tag(
            id
        ) values (
            #{id}
        )
    </insert>

</mapper>