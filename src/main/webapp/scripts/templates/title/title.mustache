<div id="breadcrumbs">
    Seeds Now! → Basil
</div>
<div id="title" class="clearfix">
    {{#context}}
    <div id="title-header">
        <span id="title-text">
                <!--Seeds Now! → Basil-->
                {{ title }}
        </span>
        <div id="title-follow" style="float: right;"></div>
    </div>
    <div id="title-body" class="clearfix">
        {{#echoedUser}}
            <ul>
                <li><strong>{{followers}}</strong> Followers</li>
                <li><strong>{{following}}</strong> Following</li>
                <li><strong>{{views}}</strong> Views</li>
                <li><strong>{{votes}}</strong> Votes</li>
            </ul>
        {{/echoedUser}}
        {{#partner}}
            <ul>
                <li><strong>{{followers}}</strong> Followers</li>
                <li><strong>{{views}}</strong> Views</li>
                <li><strong>{{votes}}</strong> Votes</li>
            </ul>
        {{/partner}}

    </div>
    <div id="title-nav" class="clearfix">
        <ul>
            <li class="selected">Stories: {{stories}}</li>
            <!--{{#context.content}}-->
                <!--<li {{#isDefault}}class="selected"{{/isDefault}}>{{type}} ({{count}})</li>-->
            <!--{{/context.content}}-->
        </ul>
    </div>
    {{/context}}
</div>