:@template"$ $mainColor: #FF7733;
$fbColor: #3B5998;
$twitterColor: #4099FF;

$labelColor: #666;
$navColor: #666;


$containerColor: #FFF;
$backgroundColor: #999;

$headerHeight: 50px;

$navFontFamily: BebasNeue, Arial, sans-serif;
$textFontFamily: HelveticaNeue-Light,Arial, Helvetica, sans-serif;

//RESET
html, body, div, h1, h2, h3, h4, h5, h6 ,ul, ol ,dl, li ,dt ,dd ,p ,blockquote,
pre, form, fieldset, table, th, td{
	margin:0;
	padding: 0;
	font-family: $textFontFamily;
}

//STANDARD HTML ELEMENTS DEFINITION
html{
    height: 100%;
}

body{
    min-height:100%;
}

.retailer{
    background-color: $backgroundColor;
}

a{
	text-decoration:none;
	color:inherit;
}

h2{
    font-family: $navFontFamily;
    color: $labelColor;
}

/*Echoed Colors*/
.echoed-text{
    color:$mainColor;
}

/*Facebook Colors*/
.fb-text,.like-text,.comment-text{
    color: $fbColor;
}

/*Twitter Colors*/
.twitter-text,.retweet-text{
    color: $twitterColor;
}

/*White Areas*/
.third,.half,.table_container{
    background-color: $containerColor;
}


#container{
	width: 100%;
    height: 100%;
    min-height: 100%;
}
	
	#header{
		height:50px;
		border-bottom: 1px solid #999;
		background-color: $containerColor;
	}
	
		#logo{	
            margin-left:15px;
			font-size:48px;
			line-height: $headerHeight;
			height: $headerHeight;
			float:left;
			font-family: $navFontFamily;
		}
		
		
		#user{
			position:absolute;
			padding:10px;
			font-size: 14px;
			right:0px;
			height: $headerHeight;
			text-align:right;
		}
		
		#header-nav{
			margin-left:25px;
			float:left;
		}
		
			#header-nav li{
				padding:5px 20px;
				height: 40px;
				line-height: $headerHeight;			
				list-style:none;
                font-size:24px;
				font-family: $navFontFamily;
                color: $navColor;
				float:left;
			}
			
			#header-nav li:hover, #header-nav .current{
				color: $mainColor;
				background-image:url(http://v1-cdn.echoed.com/arrow.png);
				background-position:bottom;
				background-repeat:no-repeat;
			}

        #sub-nav{
            height: 25px;
            background-color: $containerColor;
            text-align:center;
            line-height: 25px;
            font-family: $navFontFamily;
        }

        #sub-nav a{
            margin: 0px 10px;
            color: $navColor;
        }

        #sub-nav a:hover,#sub_nav a .current{
            color : $mainColor;
        }
			

	
	#content{
		text-align:center;
	}

	#content-selector-container{
		border-bottom: 1px solid #CCC;
		padding:10px;
		margin-bottom:10px;
	}


		#content-selector{
			font-family: $navFontFamily;
			margin: 0px auto 0px;
		}
		
			#content-selector a{
				margin:0px 10px;
			}
			
			#content-selector a:hover, {
				color: $mainColor;
			}

            a.current{
                color: $mainColor;
            }


			
	
		#exhibit{
			width:80%;
			margin:auto;
		}

        #exhibit:after{
            content: ".";
            display: block;
            height: 0;
            clear: both;
            visibility: hidden;
        }
			.exhibit_image{
				width: 230px;
 				border:3px solid #CCC;
			}
			
			
		#friends{
			width:80%;
			margin:auto;
		}
			
			
			.friend{
				width: 150px;
				height:100px;
				float:left;
			}
		
			
			.item_wrap{
				float:left;
				margin:2px;
				position:relative;
			}
			
			.item_hover{
				position:absolute;
				top:0;
				left:0;
				width:100%;
				height:100%;
				background:url(http://v1-cdn.echoed.com/blackPx.png) repeat;
				overflow:hidden;				
			}
			
			.item_hover_wrap{
				padding: 10px 10px 0px 10px;
				text-align:left;
				color: #FFF;
				overflow:hidden;
			}

        #summary-stats{

        }

            #summary-stats-column1{
                width:450px;
                margin-left:10px;
                float:left;
                font-size:36px;
            }

            #summary-stats-column2{
                width:450px;
                margin-right:10px;
                float:right;
            }

                .ss-row{
                    height:100px;
                }

                .ss-box{
                    height:100px;
                    width: 120px;
                    float:left;
                    margin: 0px 0px 20px 20px;
                }

                .ss-box-label{
                    margin-top:20px;
                    width:100%;
                    margin-bottom:5px;
                    text-align:center;
                    font-family: $navFontFamily;
                    font-size: 1.5em;
                    color: $labelColor;
                }

                .ss-box-number{
                    width:100%;
                    text-align:right;
                    font-size: 48px;
                    font-weight: bold;
                    font-family: $navFontFamily;
                }
        .report-selector{
            text-align:left;
        }

            .select-report{
                border: 1px solid #333;
                width: 200px;
                padding: 0px 10px;
                background-color: white;
                font-size: 1.5em;
                font-family: $navFontFamily;
                -webkit-appearance: none;
            }

        .product-detail-image-container{
            float:left;
            width: 150px;
            height: 200px;
        }

        .product-detail-image{
            max-width: 150px;
            max-height: 200px;
        }

        .product-detail-table-container{
            float:left;
        }

        .product-detail-table{
            font-size:14px;
            font-family: $textFontFamily;
        }

            .product-detail-table tr td{
                padding: 10px 10px 0px 0px;
                border-bottom: 1px solid #CCC;
            }

        .report-nav{
            width:200px;
            float:left;
            min-height:400px;
        }

            .report-label{
                text-align: left;
            }

        .report-chart{
            width:700px;
            padding-left:10px;
            min-height:400px;
            border-left:1px solid #ccc;
            float:left;
        }
			
//CLEARFIX

.row{
    padding: 20px 10px 0px 10px;
}

.row:after{
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}

.full{
    padding: 10px;
    width: 920px;
    min-height: 300px;
    background-color: $containerColor;
}
.full:after{
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}

.half{
    padding: 10px;
    width: 440px;
    margin-right: 20px;
    float:left;
    height: 300px;
    background-color: $containerColor;
}

    .half-chart{
        height: 250px;
    }

.label{
    font-family: $navFontFamily;
    color: $labelColor;
    font-size: 1.5em;
    text-align:left;
}

.third{
    padding: 10px;
    width: 280px;
    margin-right: 20px;
    min-height: 300px;
    float:left;
    background-color: $containerColor;
}
	
.clear{
	clear:both;
}

/*** ISOTOPE ***/

.isotope-item {
  z-index: 2;
}

.isotope-hidden.isotope-item {
  pointer-events: none;
  z-index: 1;
}

/**** Isotope CSS3 transitions ****/

.isotope,
.isotope .isotope-item {
  -webkit-transition-duration: 0.8s;
	 -moz-transition-duration: 0.8s;
		  transition-duration: 0.8s;
}

.isotope {
  -webkit-transition-property: height, width;
	 -moz-transition-property: height, width;
		  transition-property: height, width;
}

.isotope .isotope-item {
  -webkit-transition-property: -webkit-transform, opacity;
	 -moz-transition-property:    -moz-transform, opacity;
		  transition-property:         transform, opacity;
}

/**** disabling Isotope CSS3 transitions ****/

.isotope.no-transition,
.isotope.no-transition .isotope-item,
.isotope .isotope-item.no-transition {
  -webkit-transition-duration: 0s;
	 -moz-transition-duration: 0s;
		  transition-duration: 0s;
}	


.ui-button {
    -webkit-appearance:none;
    background-color:#FFF;
    height: 30px;
    border: 0px;
}
.ui-button-icon-only .ui-button-text { padding: 0.35em; }
.ui-menu-item{
    list-decoration: none;
}
.ui-autocomplete-input {
    -webkit-appearance:none;
    font-family: $navFontFamily;
    font-size: 24px;
    border:0px;
}
